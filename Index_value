#include<stdio.h>
#include<stdlib.h>
int main()
{
    int a[10],i,j,n,key;
    printf("Enter the size of array\n");
    scanf("%d",&n);
    printf("Enter the array elements\n");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("Enter the element to be searched\n");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if(a[i]==key)
        {
        printf("Element is present in the index %d",i);
        exit(0);
        }
    }
    printf(" Element not present in the array\n");
    return 0;
}
