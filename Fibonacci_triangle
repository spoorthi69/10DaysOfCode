#include<stdio.h>
void main()
{
    int n,i,j,x,y,z;
    printf("Enter the limit\n");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {  x=0;
       y=1;
       printf("%d  ",y);
      for(j=0;j<i;j++)
      {
      z=x+y;
      printf("%d  ",z);
      x=y;
      y=z;
      }
      printf("\n");
    }
}
